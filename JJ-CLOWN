                 if v.Text == "Sturdy" then
                     v.Name = "SturdyButton"
                 end
             end
         end
     
         for i,v in pairs(ScrollingFramePlus:GetChildren()) do
             if v.Name == "TextButton" then
                 if v.Text == "Rossy" then
                     v.Name = "RossyButton"
                 end
             end
         end
     
         for i,v in pairs(ScrollingFramePlus:GetChildren()) do
             if v.Name == "TextButton" then
