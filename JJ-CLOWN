                     v.Name = "Dance1Button"
                 end
             end
         end
     
         for i,v in pairs(ScrollingFrame:GetChildren()) do
             if v.Name == "TextButton" then
                 if v.Text == "Dance2" then
                     v.Name = "Dance2Button"
                 end
             end
         end
     
         for i,v in pairs(ScrollingFrame:GetChildren()) do
             if v.Name == "TextButton" then
                 if v.Text == "Greet" then
                     v.Name = "GreetButton"
                 end
             end
         end
     
         for i,v in pairs(ScrollingFrame:GetChildren()) do
             if v.Name == "TextButton" then
                 if v.Text == "Chest Pump" then
                     v.Name = "ChestPumpButton"
                 end
             end
         end
     
